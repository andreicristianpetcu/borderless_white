{
    "name": "UiPath",
    "author": "Andrei Cristian Petcu",
    "description": "UiPath theme that works well with Platform and Orchestrator",
    "homepage_url": "https://github.com/andreicristianpetcu/firefox_themes",
    "manifest_version": 2,
    "version": "1.0",
    "theme": {
        "colors": {
            "frame": "#1976d2",
            "tab_background_separator": "white",
